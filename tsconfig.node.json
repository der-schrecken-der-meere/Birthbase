{
    "compilerOptions": {
      "composite": true,
      "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
      "skipLibCheck": true,
      "module": "ESNext",
      "moduleResolution": "bundler",
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "noEmit": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"],
        "@com/*": ["./src/components/*"],
        "@hooks/*": ["./src/hooks/*"],
        "@stores/*": ["./src/stores/*"],
        "@features/*": ["./src/features/*"],
        "@apis/*": ["./src/apis/*"],
        "@lib/*": ["./src/lib/*"],
      },
      "lib": ["WebWorker"]
    },
    "include": ["vite.config.ts"],
    "exclude": ["src-tauri"]
  }