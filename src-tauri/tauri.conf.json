{
  "app": {
    "enableGTKAppId": true,
    "macOSPrivateApi": true,
    "security": {
      "assetProtocol": {
        "enable": false,
        "scope": []
      },
      "capabilities": [],
      "csp": null,
      "dangerousDisableAssetCspModification": false,
      "devCsp": null,
      "freezePrototype": false,
      "pattern": {
        "use": "brownfield"
      }
    },
    "windows": [
      {
        "closable": true,
        "decorations": true,
        "focus": true,
        "height": 600,
        "maximizable": true,
        "minHeight": 600,
        "minimizable": true,
